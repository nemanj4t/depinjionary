{
  "name": "depinjionary",
  "version": "1.1.1",
  "description": "Dependency Injection Container for TS projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nemanj4t/depinjionary.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/esm/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build-common": "tsc --project tsconfig.json",
    "build-esm": "tsc --project tsconfig.esm.json",
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.json",
    "lint": "tsc",
    "test": "jest"
  },
  "keywords": [],
  "author": "Nemanja Turanjanin",
  "license": "MIT",
  "devDependencies": {
    "@types/core-js": "^2.5.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/reflect-metadata": "^0.0.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
